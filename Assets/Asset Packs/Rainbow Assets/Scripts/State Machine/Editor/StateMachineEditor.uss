GridBackground { 
    --grid-background-color: rgb(40, 40, 40);
    --line-color: rgba(0,0,0, 0.2);
    --thick-line-color: rgba(0,0,0, 0.5);
    --spacing: 15;
}
 
.actionState {
    background-color: rgb(79, 75, 75);
}

Port {
    justify-content: center;
    position: absolute;
    align-self: center;
    width: 100%;
    height: 100%;
    scale: 1 2;
}

Port>Label{
	display:none;
}
